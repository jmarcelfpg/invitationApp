    "@types/express-session": "^1.15.8",
    "@types/handlebars": "^4.0.37",
    "@types/mailgun-js": "^0.16.0",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.0.11",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.0.3",
    "@types/nodemailer": "^4.6.0",
