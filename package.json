{
  "name": "invitation-app",
  "version": "1.0.0",
  "description": "\"Invitation to festial for UAG generation\"",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node index.js",
    "build": "webpack && tsc -p tsconfig.server.json",
    "build-watch": "concurrently \"mongod\" \"webpack -w\" \"tsc -p tsconfig.server.json -w\" \"nodemon --watch index.js --watch public --watch app --watch config \"",
    "build-devwatch": "concurrently \"mongod \" \"webpack -w\" \"tsc -p tsconfig.developmentServer.json -w\" \"nodemon ./indexdev.js -r dotenv/config --watch index.js --watch public\"",
    "build-debugwatch": "concurrently \"mongod\" \"webpack -w\" \"tsc -p tsconfig.server.json -w\" \"nodemon --inspect --watch index.js --watch public --watch app --watch config \"",
    "build-server": "tsc -p tsconfig.server.json",
    "build-serverWatch": "tsc -p tsconfig.server.json -w",
    "build-client": "webpack",
    "build-clientWatch": "webpack -w"
  },
  "repository": "https://github.com/jmarcelfpg/invitationApp.git",
  "author": "JMARCELFPG",
  "license": "MIT",
  "private": false,
